{
  "name": "@falkor/falkor-library",
  "version": "1.0.0-beta.2",
  "description": "Shared operations library",
  "author": "Barnabas Bucsy (https://github.com/theonethread)",
  "contributors": [
    "Barnabas Bucsy (https://github.com/theonethread)"
  ],
  "license": "MIT",
  "repository": "github:theonethread/falkor-library",
  "homepage": "https://falkor.world/",
  "keywords": [
    "falkor",
    "operations",
    "library"
  ],
  "type": "module",
  "main": ".dist/index.js",
  "module": ".dist/index.js",
  "typings": ".dist/index.d.ts",
  "files": [
    ".dist"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "debug": "rimraf .dist/**/* && falkor-bundler --debug",
    "release": "rimraf .dist/**/* && falkor-bundler --release",
    "doc": "rimraf .doc/**/* && typedoc --hideGenerator --includeVersion  --disableSources --excludePrivate --excludeExternals --name \"Falkor Operations Library\" --internalNamespace @falkor --out .doc src/index.ts",
    "prepublishOnly": "npm run release"
  },
  "devDependencies": {
    "@types/node": "14.17.21",
    "@types/pretty-time": "1.1.1",
    "rimraf": "3.0.2",
    "@falkor/falkor-bundler": "1.1.2",
    "typedoc": "0.22.5",
    "typedoc-plugin-rename-defaults": "0.4.0",
    "typedoc-plugin-missing-exports": "0.22.3"
  },
  "dependencies": {
    "@types/shelljs": "0.8.9",
    "@types/figlet": "1.5.4",
    "@types/semver": "7.3.8",
    "@types/node-fetch": "3.0.3",
    "chalk": "4.1.2",
    "strip-ansi": "7.0.1",
    "ansi-escapes": "5.0.0",
    "figlet": "1.5.2",
    "semver": "7.3.5",
    "pretty-time": "1.1.0",
    "shelljs": "0.8.4",
    "node-fetch": "3.0.0"
  }
}
